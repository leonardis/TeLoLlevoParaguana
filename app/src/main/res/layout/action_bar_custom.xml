<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/red_main"
    android:elevation="4dp"
    >

    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:layout_width="65dp"
            android:layout_height="55dp"
            android:src="@drawable/ic_logo_bar"
            android:paddingTop="5dp"
            android:paddingBottom="5dp"
            android:id="@+id/tlpIV"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageButton"
            android:layout_toEndOf="@+id/imageButton" />

        <ImageButton
            android:id="@+id/overButton"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_more_vert_white"/>

        <ImageButton
            android:id="@+id/clearButton"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_clear_white_24dp"/>


        <EditText
            android:id="@+id/searchEdit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/overButton"
            android:layout_marginBottom="2dp"
            android:hint="Buscar"
            android:textColorHint="@color/btn_disabled"
            android:textColor="@color/white_main"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:layout_alignTop="@+id/overButton"
            android:imeOptions="actionSearch"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageButton"
            android:layout_toEndOf="@+id/imageButton" />

        <ImageButton
            android:id="@+id/search_button"
            android:layout_width="38dp"
            android:layout_height="40dp"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_search_white"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/overButton"/>
        <ImageButton
            android:id="@+id/car_button"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_shopping_cart_white"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/search_button"
            android:layout_marginRight="5dp"/>

        <TextView
            android:id="@+id/counter"
            android:layout_width="wrap_content"
            android:minWidth="20sp"
            android:textSize="12sp"
            android:textColor="#ffffffff"
            android:layout_height="20sp"
            android:gravity="center"
            android:text="30"
            android:layout_alignTop="@id/car_button"
            android:layout_alignRight="@id/car_button"
            android:layout_marginLeft="3dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/rounded_square"/>

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/imageButton"
            android:layout_alignTop="@+id/searchEdit"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="@android:color/transparent"
            android:src="@drawable/ic_menu_white_24dp"
            android:layout_centerVertical="true"/>


    </RelativeLayout>

</android.support.v7.widget.Toolbar>
